# Server Configuration
NODE_ENV=development
LOG_LEVEL=info

# Cache Configuration (in seconds)
CACHE_DEFAULT_TTL=300          # 5 minutes
CACHE_QUOTE_TTL=5              # 5 seconds
CACHE_HISTORICAL_TTL=3600      # 1 hour
CACHE_NEWS_TTL=300             # 5 minutes
CACHE_SEARCH_TTL=3600          # 1 hour
CACHE_INDICATOR_TTL=300        # 5 minutes

# Yahoo Finance Configuration
YAHOO_FINANCE_TIMEOUT=10000    # 10 seconds
YAHOO_FINANCE_RETRY_ATTEMPTS=3
YAHOO_FINANCE_RETRY_DELAY=1000 # 1 second base delay

# Rate Limiting (optional, for future)
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60000        # 1 minute
